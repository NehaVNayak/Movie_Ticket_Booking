        <servlet-name>ManageMoviesServlet</servlet-name>
        <servlet-class>movieBooking.admin.servlet.ManageMoviesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>movieBooking.user.servlet.UserLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewMoviesServlet</servlet-name>
        <servlet-class>movieBooking.user.servlet.ViewMoviesServlet</servlet-class>
    </servlet>
    <servlet>
