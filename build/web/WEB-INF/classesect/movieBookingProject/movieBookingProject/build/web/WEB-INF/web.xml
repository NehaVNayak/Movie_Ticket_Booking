    </servlet>
    <servlet>
        <servlet-name>ViewBookingsServlet</servlet-name>
        <servlet-class>movieBooking.user.servlet.ViewBookingsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCancelServlet</servlet-name>
        <servlet-class>movieBooking.user.servlet.ViewCancelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelBookingServlet</servlet-name>
