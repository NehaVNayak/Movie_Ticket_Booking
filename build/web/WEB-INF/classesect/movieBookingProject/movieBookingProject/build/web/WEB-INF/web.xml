        <url-pattern>/admin/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageUsersServlet</servlet-name>
        <url-pattern>/admin/ManageUsersServlet</url-pattern>
    </servlet-mapping>
